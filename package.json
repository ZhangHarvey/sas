{
  "name": "node-sas",
  "version": "0.1.0",
  "description": "Data encryption package",
  "main": "lib/index.js",
  "scripts": {
    "pretest": "npm run build",
    "prepublish": "npm run build",
    "ava": "node --harmony ./node_modules/.bin/ava --verbose --serial",
    "test": "./node_modules/.bin/nyc -a --reporter=text --reporter=lcov  --reporter=html npm run ava",
    "build": "babel src -d lib",
    "lint": "eslint ./src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ZhangHarvey/sas.git"
  },
  "keywords": [
    "sas",
    "encryption",
    "ES6"
  ],
  "author": "geekwebme@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ZhangHarvey/sas/issues"
  },
  "homepage": "https://github.com/ZhangHarvey/sas",
  "engines": {
    "node": ">=4.4.3"
  },
  "dependencies": {
    "ursa": "^0.9.4"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-register": "^6.7.2",
    "coveralls": "^2.11.15",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "nyc": "^10.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "concurrency": 4,
    "failFast": false,
    "tap": false,
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "babel": "inherit"
  }
}
